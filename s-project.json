{
  "name": "notes",
  "custom" : {
    "client": {
        "bucketName": "notes.${stage}.${region}"
    },
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Amz-Date", "Authorization", "X-Api-Key"]
    }
  },
  "plugins": [
    "serverless-client-s3",
    "serverless-cors-plugin"
  ]
}
